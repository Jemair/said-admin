.app
  text-align center
  .app-header
      background-color #222
      height 150px
      padding 20px
      color white
    .app-logo
      animation: App-logo-spin infinite 20s linear;
      height: 80px;
  .app-intro
      font-size: red;

@keyframes App-logo-spin 
  from { transform: rotate(0deg); }
  to { transform: rotate(360deg); }

body,.App,#root,.ant-layout 
  height 100%
